{
	"rules":{
		"camelcase": 1,
		"comma-dangle": 2,
		"quotes": ["error", "single"],
		"semi": ["error", "never"],
		"no-undef": 0
	},

	"extends": ["eslint:recommended"],

	"parser": "babel-eslint",

	"parserOptions": {
		"ecmaVersion": 7,
		"sourceType": "module",
		"allowImportExportEverywhere": true
	}
}
